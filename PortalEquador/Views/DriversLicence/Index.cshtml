        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CurriculumId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpirationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvisionalRenewalNumber)
